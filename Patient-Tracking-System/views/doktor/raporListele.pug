doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    title Randevular
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi", crossorigin="anonymous")

  body
    .container
      h3 Rapor
      hr
      table.table.table-bordered.table-striped
        thead.table-dark
          tr
            th Rapor ID
            th Rapor Tarihi 
            th Rapor İçeriği
            th Lab ID
            th URL
        tbody
          each sonuc in result 
            tr 
              td= sonuc.rapor_id
              td= new Date(sonuc.rapor_tarihi).toLocaleDateString('tr-TR')
              td= sonuc.rapor_icerigi
              td= sonuc.lab_sonuc_id
              td
                button.btn.btn-primary(type="button", onclick=`window.location.href='${sonuc.url}'`) Git

    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js", integrity="sha384-OERcA2mIyepA7b7FbyylbRmQG6ri0f9TZUeohhV7qx54n5Qh3B/++D7GAHo43H9p", crossorigin="anonymous")
